{
  "name": "@taxee/prisma",
  "version": "1.0.0",
  "description": "Helper that lets you not have to pass DB transactions to every function.",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm run build:types && pnpm run build:js",
    "build:types": "tsc --declaration --emitDeclarationOnly",
    "build:js": "swc src --out-dir dist --strip-leading-paths",
    "lint": "biome check; status=$?; echo 'Use `pnpm run lint!` to apply the fixes.'; exit $status",
    "lint!": "biome check --write",
    "test": "ava",
    "version": "pnpm run lint!",
    "prepublishOnly": "pnpm run lint",
    "prepare": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/takagiy/taxee.js.git"
  },
  "keywords": [
    "database",
    "db",
    "transaction",
    "prisma",
    "typescript"
  ],
  "author": "Yuki Takagi <takagiy.4dev@gmail.com> (https://github.com/takagiy)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/takagiy/taxee.js/issues"
  },
  "homepage": "https://github.com/takagiy/taxee.js#readme",
  "packageManager": "pnpm@9.4.0+sha256.b6fd0bfda555e7e584ad7e56b30c68b01d5a04f9ee93989f4b93ca8473c49c74",
  "ava": {
    "files": [
      "tests/**/*.ts"
    ],
    "extensions": [
      "ts"
    ],
    "nodeArguments": [
      "-r",
      "@swc-node/register"
    ]
  },
  "dependencies": {
    "taxee": "workspace:*"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@swc-node/register": "^1.10.0",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.6.6",
    "@types/node": "^20.14.9",
    "ava": "^6.1.3",
    "typescript": "^5.5.2"
  }
}
